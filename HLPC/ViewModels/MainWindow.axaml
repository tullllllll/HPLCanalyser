<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"

        xmlns:local="clr-namespace:HLPC.Views"
        xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="HLPC.MainWindow"
        Title="HLPC"
        x:DataType="local:SomeViewModel">

	<Window.DataContext>
		<local:SomeViewModel/>
	</Window.DataContext>

	<StackPanel>
		<TextBlock Text="{Binding tekst}" HorizontalAlignment="Center"/>
		<lvc:CartesianChart Height="300" Width="500" HorizontalAlignment="Center" VerticalAlignment="Center" Background="White"
            Series="{Binding Series}">
		</lvc:CartesianChart>
	</StackPanel>



</Window>
